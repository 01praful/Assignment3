        "#Write a Python function to sum all the numbers in a list.\n",
        "\n"
      ],
      "metadata": {
        "id": "LGR25j4VvOnF"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "def sum_of_list(l):\n",
        "  total = 0\n",
        "  for i in l:\n",
        "    total = total + i\n",
        "  return total\n",
        "\n",
        "my_list = [6,3,1,5,8]\n",
        "print (sum_of_list(my_list))"
